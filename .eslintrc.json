{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
      "plugin:react-hooks/recommended",
      "prettier",
      "prettier/react"
    ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react", "react-hooks"],
  "rules": {},
  "ignorePatterns": ["dist/*"]
}
